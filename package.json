{
  "name": "swiper",
  "version": "1.0.0",
  "description": "a H5 swiper",
  "main": "src/swiper.ts",
  "repository": "http://github.com/fex-team/swiper",
  "scripts": {
    "build": "webpack -p",
    "dev": "webpack --watch",
    "test": "jest",
    "coveralls": "cat ./__coverage__/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/tests/__setupFiles__/rAF.js",
    "transform": {
      ".ts": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "moduleNameMapper": {
      "\\.css$": "<rootDir>/tests/__mocks__/styleMock.ts"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|js)$",
    "collectCoverage": true,
    "mapCoverage": true,
    "collectCoverageFrom" : ["src/**/*.ts"],
    "coverageDirectory": "__coverage__",
    "moduleFileExtensions": [
      "ts",
      "js"
    ]
  },
  "author": "zhangbobell <zhangbobell@163.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^20.0.2",
    "coveralls": "^2.13.1",
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^2.1.2",
    "jest": "^20.0.4",
    "style-loader": "^0.18.2",
    "ts-jest": "^20.0.6",
    "ts-loader": "^2.2.0",
    "typescript": "^2.3.4",
    "webpack": "^3.0.0"
  }
}
